create Table Buch(
	genre varchar(20), 
	ausgewählt bool, 
	varchar path(255)
);

create Table Termine(
	datum date, 
	uhrzeit time, 
	beschreibung varchar(100), 
	ort varchar(50), 
	hinweis varchar(100)
);

create Table Pillen(
	name varchar(50),
	montag bool, 
	dienstag bool, 
	mittwoch bool, 
	donnerstag bool, 
	freitag bool, 
	samstag bool, 
	sonntag bool, 
	zeit time
);
